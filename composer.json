{
  "name": "digitonic/mediatonic-filament",
  "description": "The MediaTonic Field is a custom input component designed specifically for ingesting pictures and returning an appropriate CDN link for them",
  "keywords": [
    "laravel",
    "filament"
  ],
  "homepage": "https://github.com/digitonic/mediatonic-filament",
  "license": "MIT",
  "authors": [
  ],
  "require": {
    "php": "^8.2|^8.3|^8.4",
    "filament/filament": "^4.0",
    "saloonphp/saloon": "^3.0",
    "spatie/laravel-package-tools": "^1.92"
  },
  "require-dev": {
    "laravel/framework": "^12.0",
    "larastan/larastan": "^3.7",
    "laravel/pint": "^1.21",
    "orchestra/testbench": "^9.0|^10.0",
    "pestphp/pest": "^3.8",
    "pestphp/pest-plugin-arch": "^3.1",
    "pestphp/pest-plugin-laravel": "^3.0",
    "phpstan/extension-installer": "^1.4"
  },
  "autoload": {
    "psr-4": {
      "Digitonic\\Mediatonic\\Filament\\": "src"
    },
    "files": [
      "src/Helpers/MediatonicHelper.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Digitonic\\Mediatonic\\Filament\\Tests\\": "tests"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Digitonic\\Mediatonic\\Filament\\MediatonicServiceProvider"
      ]
    }
  },
  "scripts": {
    "post-autoload-dump": [
      "@php ./vendor/bin/testbench icons:cache",
      "@php ./vendor/bin/testbench package:discover --ansi"
    ],
    "pint": "vendor/bin/pint -p",
    "pint-ci": "vendor/bin/pint --test -p",
    "pest": "php artisan test --parallel",
    "pest-ci": "php artisan test --parallel",
    "stan": "vendor/bin/phpstan analyse --memory-limit=1G",
    "stan-ci": "vendor/bin/phpstan analyse --no-progress --error-format=github --memory-limit=1G",
    "cov": "herd coverage ./vendor/bin/pest --coverage",
    "test": [
      "@pint",
      "@stan",
      "@pest"
    ]
  },
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "pestphp/pest-plugin": true,
      "phpstan/extension-installer": true
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
